{
    "editor.tabSize": 2,
    "editor.formatOnSave": true,
    "editor.wordWrap": "on",
    "[typescript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "hjson-formatter.options": {
        "separator": false,
        "condense": 0,
        "bracesSameLine": true,
        "quotes": "min",
        "space": 2,
        "multiline": "std"
    }
  }